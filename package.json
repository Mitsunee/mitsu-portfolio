{
  "name": "www.mitsunee.com",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "simple-git-hooks",
    "format": "prettier -w .",
    "lint": "eslint .",
    "dev": "node devServer.js",
    "build": "vite build && vite build --ssr",
    "postbuild": "vite-plugin-ssr prerender && node postBuild.js"
  },
  "nano-staged": {
    "**/*.{js,jsx}": [
      "eslint",
      "prettier -w"
    ],
    "**/*.{css,json,md}": [
      "prettier -w"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "yarn nano-staged"
  },
  "dependencies": {
    "@foxkit/node-util": "^0.4.0",
    "@preact/preset-vite": "^2.3.0",
    "cheerio": "^1.0.0-rc.12",
    "classcat": "^5.0.4",
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "globby": "^13.1.2",
    "iconoir": "^5.1.4",
    "preact": "^10.10.6",
    "preact-render-to-string": "^5.2.2",
    "rehype": "^12.0.1",
    "rehype-external-links": "^2.0.0",
    "rehype-preset-minify": "^6.0.0",
    "vite": "^2.9.13",
    "vite-plugin-ssr": "^0.3.64"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "eslint": "^8.22.0",
    "eslint-config-foxkit": "^1.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nano-staged": "^0.8.0",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.8.0",
    "prettier": "^2.7.1",
    "simple-git-hooks": "^2.8.0"
  }
}

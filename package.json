{
  "name": "mitsuwww",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "simple-git-hooks",
    "format": "prettier -w .",
    "lint": "eslint .",
    "dev": "astro dev",
    "typecheck": "astro check && tsc --noEmit",
    "prebuild": "yarn lint && yarn typecheck",
    "build": "astro build"
  },
  "nano-staged": {
    "**/*.{js,jsx,ts,tsx,css,astro,json,md,yml}": [
      "prettier -w"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "yarn nano-staged"
  },
  "dependencies": {
    "@astrojs/preact": "^1.2.0",
    "@fontsource/atkinson-hyperlegible": "^4.5.11",
    "@fontsource/lexend": "^4.5.14",
    "@foxkit/node-util": "^0.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "astro": "^1.9.2",
    "classcat": "^5.0.4",
    "eslint": "^8.32.0",
    "eslint-config-foxkit": "^2.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-astro": "^0.21.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "iconoir": "^6.0.0",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.8.3",
    "preact": "^10.6.5",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "nano-staged": "^0.8.0",
    "prettier": "^2.8.3",
    "simple-git-hooks": "^2.8.1"
  }
}

{
  "name": "mitsuwww",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "simple-git-hooks",
    "format": "prettier -w . --plugin=prettier-plugin-astro",
    "format:check": "prettier -c . --plugin=prettier-plugin-astro",
    "lint": "eslint .",
    "lint:strict": "eslint --max-warnings 0 .",
    "dev": "astro dev",
    "typecheck": "astro check && tsc --noEmit",
    "build": "astro build"
  },
  "nano-staged": {
    "**/*.{js,jsx,ts,tsx,css,json,md,yml}": [
      "prettier -w"
    ],
    "**/*.astro": [
      "prettier -w --plugin=prettier-plugin-astro"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "browserslist": [
    "last 2 versions",
    ">0.5% and not dead"
  ],
  "dependencies": {
    "@astrojs/preact": "^3.0.0",
    "@csstools/postcss-global-data": "^2.1.0",
    "@fontsource/atkinson-hyperlegible": "^5.0.2",
    "@fontsource/lexend": "^5.0.2",
    "@foxkit/node-util": "^0.4.0",
    "@types/node": "^18.15.1",
    "astro": "^3.1.2",
    "classcat": "^5.0.4",
    "iconoir": "^6.8.0",
    "postcss": "^8.4.24",
    "postcss-preset-env": "^9.1.4",
    "preact": "^10.15.1",
    "typescript": "~5.2.2"
  },
  "devDependencies": {
    "@astrojs/check": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.42.0",
    "eslint-config-foxkit": "^3.0.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nano-staged": "^0.8.0",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "simple-git-hooks": "^2.8.1"
  }
}

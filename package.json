{
  "name": "mitsuwww",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "simple-git-hooks",
    "format": "prettier -w .",
    "lint": "eslint .",
    "dev": "astro dev",
    "typecheck": "astro check && tsc --noEmit",
    "prebuild": "yarn lint && yarn typecheck",
    "build": "astro build"
  },
  "nano-staged": {
    "**/*.{js,jsx,ts,tsx,css,astro,json,md,yml}": [
      "prettier -w"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "yarn nano-staged"
  },
  "browserslist": [
    "last 2 versions",
    ">0.5% and not dead"
  ],
  "dependencies": {
    "@astrojs/preact": "^2.2.1",
    "@csstools/postcss-global-data": "^1.0.3",
    "@fontsource/atkinson-hyperlegible": "^5.0.2",
    "@fontsource/lexend": "^5.0.2",
    "@foxkit/node-util": "^0.4.0",
    "@types/node": "^18.15.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "astro": "^2.5.7",
    "classcat": "^5.0.4",
    "eslint": "^8.42.0",
    "eslint-config-foxkit": "^2.1.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-astro": "^0.27.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "iconoir": "^6.8.0",
    "postcss": "^8.4.24",
    "postcss-preset-env": "^8.4.2",
    "preact": "^10.15.1",
    "typescript": "5.0.x"
  },
  "devDependencies": {
    "nano-staged": "^0.8.0",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1"
  }
}

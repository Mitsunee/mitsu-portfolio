{
  "name": "mitsuwww",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "simple-git-hooks",
    "format": "prettier -w .",
    "lint": "eslint .",
    "dev": "astro dev",
    "typecheck": "astro check && tsc --noEmit",
    "prebuild": "yarn lint && yarn typecheck",
    "build": "astro build",
    "postbuild": "node src/scripts/postBuild.js"
  },
  "nano-staged": {
    "**/*.{js,jsx}": [
      "eslint",
      "prettier -w"
    ],
    "**/*.{css,json,md}": [
      "prettier -w"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "yarn nano-staged"
  },
  "dependencies": {
    "@astrojs/preact": "^1.2.0",
    "@fontsource/atkinson-hyperlegible": "^4.5.11",
    "@fontsource/lexend": "^4.5.14",
    "@foxkit/node-util": "^0.4.0",
    "astro": "^1.9.2",
    "cheerio": "^1.0.0-rc.12",
    "classcat": "^5.0.4",
    "globby": "^13.1.2",
    "iconoir": "^5.3.0",
    "preact": "^10.6.5",
    "rehype": "^12.0.1",
    "rehype-preset-minify": "^6.0.0"
  },
  "devDependencies": {
    "eslint": "^8.23.0",
    "eslint-config-foxkit": "^1.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nano-staged": "^0.8.0",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.8.1",
    "prettier": "^2.7.1",
    "simple-git-hooks": "^2.8.0"
  }
}

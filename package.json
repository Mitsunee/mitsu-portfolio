{
  "name": "mitsuwww",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "simple-git-hooks",
    "format": "prettier -w .",
    "lint": "eslint .",
    "dev": "astro dev",
    "typecheck": "astro check && tsc --noEmit",
    "prebuild": "yarn lint && yarn typecheck",
    "build": "astro build"
  },
  "nano-staged": {
    "**/*.{js,jsx,ts,tsx,css,astro,json,md,yml}": [
      "prettier -w"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "yarn nano-staged"
  },
  "browserslist": [
    "last 2 versions",
    ">0.5% and not dead"
  ],
  "dependencies": {
    "@astrojs/preact": "^2.1.0",
    "@csstools/postcss-global-data": "^1.0.3",
    "@fontsource/atkinson-hyperlegible": "^4.5.11",
    "@fontsource/lexend": "^4.5.15",
    "@foxkit/node-util": "^0.4.0",
    "@types/node": "^18.15.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "astro": "^2.1.2",
    "classcat": "^5.0.4",
    "eslint": "^8.36.0",
    "eslint-config-foxkit": "^2.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-astro": "^0.24.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "iconoir": "^6.4.0",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^8.0.1",
    "preact": "^10.13.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "nano-staged": "^0.8.0",
    "prettier": "^2.8.4",
    "simple-git-hooks": "^2.8.1"
  }
}

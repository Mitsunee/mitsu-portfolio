{
  "name": "mitsuwww",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "simple-git-hooks",
    "format": "prettier -w . --plugin=prettier-plugin-astro",
    "format:check": "prettier -c . --plugin=prettier-plugin-astro",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:strict": "eslint --max-warnings 0 .",
    "dev": "astro dev",
    "typecheck": "astro check && tsc --noEmit",
    "build": "astro build"
  },
  "nano-staged": {
    "**/*.{js,jsx,ts,tsx,css,json,md,yml}": [
      "prettier -w"
    ],
    "**/*.astro": [
      "prettier -w --plugin=prettier-plugin-astro"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "browserslist": [
    "last 2 versions",
    ">0.5% and not dead"
  ],
  "dependencies": {
    "@astrojs/preact": "^3.1.1",
    "@csstools/postcss-global-data": "^2.1.1",
    "@fontsource/atkinson-hyperlegible": "^5.0.18",
    "@fontsource/lexend": "^5.0.18",
    "@foxkit/node-util": "^0.6.0",
    "@nanostores/preact": "^0.5.1",
    "@types/node": "^18.19.18",
    "astro": "^4.4.3",
    "classcat": "^5.0.4",
    "iconoir-react": "^7.4.0",
    "nanostores": "^0.10.0",
    "postcss": "^8.4.35",
    "postcss-preset-env": "^9.4.0",
    "preact": "^10.19.6",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "sharp": "^0.33.2",
    "typescript": "~5.3.3"
  },
  "devDependencies": {
    "@astrojs/check": "^0.5.5",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-foxkit": "^3.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "nano-staged": "^0.8.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "simple-git-hooks": "^2.8.1"
  }
}

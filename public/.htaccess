RewriteEngine on
ErrorDocument 404 /error/404/index.html
ErrorDocument 500 /error/500/index.html
ErrorDocument 400 /error/400/index.html

# FGO Site Legacy
RewriteRule ^fgo/assets/(.*)$ fgo-exp-ce/assets/$1
RewriteRule ^fgo-time/?$ /fgo-moved/index.html [L]
RewriteRule ^fgo/upgrades/?$ /fgo-moved/index.html [L]
RewriteRule ^fgo/ap/?$ /fgo-moved/index.html [L]
RewriteRule ^fgo/(.*)$ /fgo-moved/index.html [L]
